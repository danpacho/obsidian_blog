        "test": "vitest",
        "test:ci": "vitest run",
        "lint:ts": "tsc --noEmit"
    },
    "dependencies": {
        "@obsidian_blogger/helpers": "workspace:^",
        "boxen": "^7.1.1",
        "chalk": "^5.3.0"
