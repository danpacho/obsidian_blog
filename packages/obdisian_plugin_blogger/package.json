{
    "name": "@blogger/obsidian_plugin_blogger",
    "version": "0.1.0",
    "description": "Obsidian plugin for extract vault to contents.",
    "author": "danpacho",
    "license": "MIT",
    "keywords": [],
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "files": [
        "dist"
    ],
    "main": "./dist/index.js",
    "module": "./dist/index.mjs",
    "types": "./dist/index.d.ts",
    "scripts": {
        "build:script": "tsup --config=script.tsup.config.ts",
        "build:plugin": "tsup --config=plugin.tsup.config.ts",
        "dev": "concurrently \"pnpm build:script\" \"pnpm build:plugin\" \"node ./dist/hot_reload.mjs --parallel\""
    },
    "dependencies": {
        "@blogger/build_system": "*workspaces",
        "@blogger/helpers": "workspace:^",
        "@blogger/logger": "workspace:^",
        "builtin-modules": "^3.3.0",
        "obsidian": "1.5.7-1"
    },
    "devDependencies": {
        "watcher": "^2.3.0"
    }
}
