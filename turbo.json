{
    "$schema": "https://turbo.build/schema.json",
    "ui": "tui",
    "tasks": {
        "build": {
            "persistent": true
        },
        "test": {
            "dependsOn": ["^test"],
            "outputs": []
        },
        "lint": {
            "outputs": []
        },
        "dev": {
            "persistent": true,
            "cache": false
        },
        "start": {
            "cache": false
        },
        "clean": {
            "cache": false
        }
    }
}
